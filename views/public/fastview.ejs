<div id="product-pop-up" style="display: none; width: 700px;">
            <div class="product-page product-pop-up">
              <div class="row">
                <div class="col-md-6 col-sm-6 col-xs-3">
                  <div id="fproduct-main-images" class="product-main-image">
                     <img id="fpic" src="" alt="Cool green dress with red bell"  class="img-responsive">
                  </div>
                  <div id="fproduct-other-images" class="product-other-images">
                  </div>
                </div>
                <div class="col-md-6 col-sm-6 col-xs-9">
                  <h1 id="fproductName"></h1>
                  <div class="price-availability-block clearfix">
                    <div class="price">
                      <strong><span>$</span><div class="inline" id="fdifferPrice"></div></strong>
                      <em>$<span id="fmarketPrice"></span></em>
                    </div>
                    <div class="availability">
                      <strong id="fonsale" class="bg-red"></strong>
                    </div>
                  </div>
                  <div id="fdescription" class="description">
                  </div>
                  <div class="product-page-options">
                    
                    <div class="pull-left">
                      <label class="control-label">选择:</label>
                      <select id="fdiffer"  class="form-control input-sm">
                      </select>
                    </div>
                  </div>
                  <div class="product-page-cart">
                    <div class="product-quantity">
                        <input id="fproduct-quantity" type="text" value="1" name="product-quantity" class="form-control input-sm">
                    </div>
                    <button id="fcart" class="btn btn-primary" type="submit">加入购物车</button>
                    <a href="" id="fbiu" class="btn btn-default">商品详情</a>
                  </div>
                </div>

                <div class="sticker sticker-sale"></div>
              </div>
            </div>
    </div>