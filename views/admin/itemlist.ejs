<!-- BEGIN PAGE CONTAINER -->
<div class="page-container">
    <!-- BEGIN PAGE HEAD -->
    <div class="page-head">
        <div class="container">
            <!-- BEGIN PAGE TITLE -->
            <div class="page-title">
                <h1>商品管理<small>商品 列表</small></h1>
            </div>
            <!-- END PAGE TITLE -->
            <!-- BEGIN PAGE TOOLBAR -->

            <!-- END PAGE TOOLBAR -->
        </div>
    </div>
    <!-- END PAGE HEAD -->
    <!-- BEGIN PAGE CONTENT -->
    <div class="page-content">
        <div class="container">
            <!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->

            <!-- END SAMPLE PORTLET CONFIGURATION MODAL FORM-->
            <!-- BEGIN PAGE BREADCRUMB -->
            <ul class="page-breadcrumb breadcrumb">
                <li>
                    <a href="#">商城后台</a><i class="fa fa-circle"></i>
                </li>
                <li>
                    <a href="#">管理中心</a>
                    <i class="fa fa-circle"></i>
                </li>
                <li class="active">
                    商品 管理
                </li>
            </ul>
            <!-- END PAGE BREADCRUMB -->
            <!-- BEGIN PAGE CONTENT INNER -->
            <div class="row">
                <div class="col-md-12">

                    <!-- Begin: life time stats -->
                    <div class="portlet light">
                        <div class="portlet-title">
                            <div class="caption">
                                <i class="fa fa-gift font-green-sharp"></i>
                                <span class="caption-subject font-green-sharp bold uppercase">商品</span>
                                <span class="caption-helper">商品管理...</span>
                            </div>
                        </div>
                        <div class="portlet-body">
                            <input id="csrf" type="text" name="_csrf" value="<%=_csrf%>" class="hidden" />
                            <div class="table-container">
                                <div class="table-actions-wrapper">
									<span>
									</span>
                                    <select class="table-group-action-input form-control input-inline input-small input-sm">
                                        <option value="">选择操作...</option>
                                        <option value="1">上架</option>
                                        <option value="2">下架</option>
                                        <option value="0">删除</option>
                                    </select>
                                    <button class="btn btn-sm yellow table-group-action-submit"><i class="fa fa-check"></i> 执行</button>
                                </div>
                                <table class="table table-striped table-bordered table-hover" id="datatable_products">
                                    <thead>
                                    <tr role="row" class="heading">
                                        <th width="1%">
                                            <input type="checkbox" class="group-checkable">
                                        </th>
                                        <th width="10%">
                                            商品唯一码
                                        </th>
                                        <th width="15%">
                                            商品名&nbsp;
                                        </th>
                                        <th width="15%">
                                            品牌&nbsp;
                                        </th>
                                        <th width="10%">
                                            浏览量
                                        </th>
                                        <th width="10%">
                                            已售
                                        </th>
                                        <th width="10%">
                                            余量
                                        </th>
                                        <th width="15%">
                                            促销时间&nbsp;
                                        </th>
                                        <th width="10%">
                                            当前状态
                                        </th>
                                        <th width="10%">
                                            更新商品
                                        </th>
                                    </tr>
                                    <tr role="row" class="filter">
                                        <td>
                                        </td>
                                        <td>
                                            <input type="text" class="form-control form-filter input-sm" name="query[id]">
                                        </td>
                                        <td>
                                            <input type="text" class="form-control form-filter input-sm" name="query[name]">
                                        </td>
                                        <td>
                                            <input type="text" class="form-control form-filter input-sm" name="query[BrandId]">
                                        </td>
                                        <td>
                                            <div class="margin-bottom-5">
                                                <input type="text" class="form-control form-filter input-sm" name="query[pv][gt]" placeholder="From"/>
                                            </div>
                                            <input type="text" class="form-control form-filter input-sm" name="query[pv][lt]" placeholder="To"/>
                                        </td>
                                        <td>
                                            <div class="margin-bottom-5">
                                                <input type="text" class="form-control form-filter input-sm" name="query[sold][pv]" placeholder="From"/>
                                            </div>
                                            <input type="text" class="form-control form-filter input-sm" name="query[sold][pv]" placeholder="To"/>
                                        </td>
                                        <td>
                                            <div class="margin-bottom-5">
                                                <input type="text" class="form-control form-filter input-sm" name="query[inhand][gt]" placeholder="From"/>
                                            </div>
                                            <input type="text" class="form-control form-filter input-sm" name="query[ihand][lt]" placeholder="To"/>
                                        </td>
                                        <td>
                                            <div class="input-group date date-picker margin-bottom-5" data-date-format="yyyy-mm-dd">
                                                <input type="text" class="form-control form-filter input-sm" readonly name="query[saleStart][lt]" placeholder="From">
											<span class="input-group-btn">
											<button class="btn btn-sm default" type="button"><i class="fa fa-calendar"></i></button>
											</span>
                                            </div>
                                            <div class="input-group date date-picker" data-date-format="yyyy-mm-dd">
                                                <input type="text" class="form-control form-filter input-sm" readonly name="query[saleStart][gt] " placeholder="To">
											<span class="input-group-btn">
											<button class="btn btn-sm default" type="button"><i class="fa fa-calendar"></i></button>
											</span>
                                            </div>
                                        </td>
                                        <td>
                                            <select name="product_status" class="form-control form-filter input-sm">
                                                <option value="1">筛选...</option>
                                                <option value="1">上架</option>
                                                <option value="2">下架</option>
                                                <option value="0">删除</option>
                                            </select>
                                        </td>
                                        <td>
                                            <div class="margin-bottom-5">
                                                <button class="btn btn-sm yellow filter-submit margin-bottom"><i class="fa fa-search"></i> 查询</button>
                                            </div>
                                            <button class="btn btn-sm red filter-cancel"><i class="fa fa-times"></i> 重置</button>
                                        </td>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <!-- End: life time stats -->
                </div>
            </div>
            <!-- END PAGE CONTENT INNER -->
