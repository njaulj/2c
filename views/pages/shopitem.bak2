    <script type="text/javascript">
        var dselect=[]
        jQuery(document).ready(function() {
            Layout.init();    
            Layout.initOWL();
            Layout.initTwitter();
            Layout.initImageZoom();
            Layout.initTouchspin();
            Layout.initUniform();
            Layout.initFixHeaderWithPreHeader();

            $("#popnew").hover(function(){
               t=setTimeout(function(){

                Newp.init()},200)
            },
            function(){

              clearTimeout(t)
            }
            )       

            $(".top-cart-block").hover(function(){
                t=setTimeout(function(){

                Ecart.init()},800)
            },
            function(){
              clearTimeout(t)
            }
            ) 


             

            $("#differ").change(function(){
               var differ=$("#differ").find("option:selected");
               var did=differ.data("did")
                        

               if(inarray(did,dselect)==2){
                  $("#cart").attr("disabled",true)
               }else{
                $("#cart").attr("disabled",false)
               }

               $("#differPrice").text((differ.data("price")/100).toFixed(2))
               $("#marketPrice").text((differ.data("market")/100).toFixed(2))
             //  $(".zoomImg").attr("src","http://7xioyb.com1.z0.glb.clouddn.com/@/product/"+differ.data("pic"))
                $(".zoomImg").remove()
               $("#pic").attr("src","http://7xioyb.com1.z0.glb.clouddn.com/@/product/"+differ.data("pic")+"?imageView2/1/w/382/h/509/interlace/1")

               $("#pic").attr("data-BigImgsrc","http://7xioyb.com1.z0.glb.clouddn.com/@/product/"+differ.data("pic"))

               $("#onsale").text(parseFloat(differ.data("price")/differ.data("market")*10).toFixed(1)+'折')
                Layout.initImageZoom();

            })

  

            $("#cart").click(function(){
               var differ=$("#differ").find("option:selected");
              
               
               pid=differ.data("pid")
               bid=differ.data("bid")
               did=differ.data("did")
               dselect.push(did)
               console.log(dselect)
               
               num=$("#product-quantity").val()


               $.ajax({
                url:'/addCart/'+bid+'/'+pid+'/'+did+'/'+num,
                type:'get',
                success:function(data){
                   // alert('good')
                   $("#cart").attr("disabled",true)
                },
                error:function(data){
                   // alert('error')

                }
               })
            })
           

        });

    
        


        function v(){
           $(function() {  
        $(".del-goods").bind('click',function(){
             var cid=$(this).data("cid")
             var num=$(this).data("num")
             var did=$(this).data("did")
             var holder=$(this)
              $.ajax({
                type:'post',
                url:'/cart/del/'+cid+'/1',
                headers:{
                  'x-csrf-token':$("#csrf").val()
                },
                success:function(result){
                    var price=parseInt(holder.prev().text().substr(1))*100
                    var subtotal=price*num

                    var evertotal=parseInt($(".top-cart-info-value").text())*100
                    console.log(evertotal)
                    var nowtotal=evertotal-subtotal

                    $(".top-cart-info-value").text((nowtotal/100).toFixed(2))

                    var s="#"+cid
                    dselect.pop(did)
                    console.log(dselect)

                    $(s).remove()

                }
              })
        
        })


        $(".add2cart").bind('click',function(){
                   var differ =$(this)
                   console.log($(this))

                   pid=differ.data("pid")
                   bid=differ.data("bid")
                   did=differ.data("did")

                  num=1
                  
                
                  console.log(pid+bid+did)


                   $.ajax({
                        url:'/addCart/'+bid+'/'+pid+'/'+did+'/'+num,
                        type:'get',
                        success:function(data){
                           // alert('good')
                           differ.attr("disabled",true)
                           differ.text("")
                        },
                        error:function(data){
                           // alert('error')

                        }
                       })



              }) 
        })
        } 

        function inarray(a,b){
          for (i in b){
            if(b[i]==a){
              return 2
            }
          }
          return 1
        }
