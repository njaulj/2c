<body>
<!-- BEGIN HEADER -->
<div class="header">
      <div class="container">
        <a class="site-logo" href="/"><img src="../../assets/global/img/logo-02.png" height=120px width=120px alt="石化英语风采大赛"></a>

       
        <!--END CART -->

      </div>
    </div>
<!-- END HEADER -->
<!-- BEGIN PAGE CONTAINER -->
<div class="page-container">
	<!-- BEGIN PAGE HEAD -->
<input class="hidden" name="_csrf" id="csrf" value="<%=_csrf%>" />
	<!-- END PAGE HEAD -->
	<!-- BEGIN PAGE CONTENT -->
	<div class="page-content">
		<div class="container">
			<!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->
		
			<!-- /.modal -->
			<!-- END SAMPLE PORTLET CONFIGURATION MODAL FORM-->
			<!-- BEGIN PAGE BREADCRUMB -->
			<ul class="page-breadcrumb breadcrumb">
				<li>
					<a href="#">主页</a><i class="fa fa-circle"></i>
				</li>
				<li>
					<a href="extra_profile.html">比赛</a>
					<i class="fa fa-circle"></i>
				</li>
				<li class="active">
					 评委评分
				</li>
			</ul>
			<!-- END PAGE BREADCRUMB -->
			<!-- BEGIN PAGE CONTENT INNER -->
			<div class="row margin-top-10">
				<div class="col-md-12 col-sm-12 col-xs-12">
					<!-- BEGIN PROFILE SIDEBAR -->
					<div class="profile-sidebar" style="width: 320px;">
						<!-- PORTLET MAIN -->
						<div class="portlet light profile-sidebar-portlet">
							<!-- SIDEBAR USERPIC -->
							<div class="profile-userpic">
								<img src="../../assets/admin/pages/media/profile/profile_user.jpg" class="img-responsive" alt="">
							</div>
							<!-- END SIDEBAR USERPIC -->
							<!-- SIDEBAR USER TITLE -->
							<div class="profile-usertitle">
								<div class="profile-usertitle-name">
									 <%=score.name%>
								</div>
								<div class="profile-usertitle-job">
									 No.<%=score.comNo%>
								</div>	
							</div>
							<!-- END SIDEBAR USER TITLE -->
							<!-- SIDEBAR BUTTONS -->
							<!-- END SIDEBAR BUTTONS -->
							<!-- SIDEBAR MENU -->
							<div class="profile-usermenu">
								<ul class="nav">
									<li class="active">
										<a href="javascript:;">
										<i class="icon-info"></i>
										《<%=score.title%>》</a>
									</li>
									
								</ul>
							</div>
							<!-- END MENU -->
						</div>
						<!-- END PORTLET MAIN -->
						<!-- PORTLET MAIN -->
					
						<!-- END PORTLET MAIN -->
					</div>
					<!-- END BEGIN PROFILE SIDEBAR -->
					<!-- BEGIN PROFILE CONTENT -->
					<div class="profile-content">
						<div class="row">
							<div class="col-md-6 col-sm-12 col-xs-12">
								<!-- BEGIN PORTLET -->
								<div class="portlet light ">
									<div class="portlet-title">
										<div class="caption caption-md">
											<span class="font-blue-madison bold uppercase">评分系统</span>
										</div>
										
									</div>
									<div class="portlet-body">
										
											<div id="calculator" data-id=<%=score.comNo%> data-teacher=<%=teacher%>>
												<!-- Screen and clear key -->
												<div class="top">
													<span class="clear">C</span>
													<div class="screen"></div>
												</div>
												<div class="keys">
													<!-- operators and other keys -->
													<span>7</span>
													<span>8</span>
													<span>9</span>
													<span class="hidden operator">+</span>
													<span>4</span>
													<span>5</span>
													<span>6</span>
													<span class="hidden operator">-</span>
													<span>1</span>
													<span>2</span>
													<span>3</span>
													<span class="hidden operator">÷</span>
													<span>0</span>
													<span>.</span>
													<span class="eval">提交</span>
													<span class="hidden operator">*</span>

												</div>
											</div>

									</div>
								</div>
								<!-- END PORTLET -->
							</div>
							<div class="col-md-6 col-sm-12 col-xs-12">
								<!-- BEGIN PORTLET -->
								<div class="portlet light ">
									<div class="portlet-title">
										<div class="caption caption-md">
											<i class="icon-bar-chart theme-font hide"></i>
											<span class="caption-subject font-blue-madison bold uppercase">排行榜</span>
										</div>
										
									</div>
									<div class="portlet-body">
									
										<div class="table-scrollable table-scrollable-borderless">
											<table class="table table-hover table-light">
											<thead>
											<tr class="uppercase">
												<th colspan="2">
													 选手
												</th>
												<th colspan="2">
													 得分情况
												</th>
												<th>
													 最终得分
												</th>
												<th>
													 排名
												</th>
											</tr>
											</thead>

											<%scores.forEach(function(score,index){%>
											<tr>
												<td class="fit">
													<%=score.comNo%>
												</td>
												<td>
													<a href="javascript:;"><%=score.name%></a>
												</td>
												<td colspan="2">
													 <%=score.scores%>
												</td>
												<td>
													 <%=score.score%>
												</td>
												<td>
													<span class="bold theme-font"><%=index+1%></span>
												</td>
											</tr>
											<%})%>
											
											</table>
										</div>
									</div>
								</div>
								<!-- END PORTLET -->
							</div>
							
						</div>
					</div>
					<!-- END PROFILE CONTENT -->
				</div>
			</div>

			<!-- END PAGE CONTENT INNER -->
		</div>
	</div>
	<!-- END PAGE CONTENT -->
</div>
